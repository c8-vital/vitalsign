<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HistoryData">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="44dp"
        android:text="History Data"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/historyData"
        app:layout_constraintStart_toStartOf="@+id/historyData" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/historyData"
        android:layout_width="361dp"
        android:layout_height="596dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.8" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginLeft="68dp"
        android:layout_marginBottom="4dp"
        android:text="Time"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/historyData"
        app:layout_constraintStart_toStartOf="@+id/historyData" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="140dp"
        android:layout_marginRight="140dp"
        android:layout_marginBottom="4dp"
        android:text="Tem"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/historyData"
        app:layout_constraintEnd_toEndOf="@+id/historyData" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="72dp"
        android:layout_marginRight="72dp"
        android:layout_marginBottom="4dp"
        android:text="SpO2"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/historyData"
        app:layout_constraintEnd_toEndOf="@+id/historyData" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:layout_marginBottom="4dp"
        android:text="Pul"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/historyData"
        app:layout_constraintEnd_toEndOf="@+id/historyData" />

</androidx.constraintlayout.widget.ConstraintLayout>